					</appinfo>
				</annotation>
			</enumeration>
			<enumeration value="FILE_EXISTS_VALIDATOR">
				<annotation>
					<appinfo>
						<jaxb:typesafeEnumMember name="FileExistsValidatorRule" />
					</appinfo>
				</annotation>
			</enumeration>
			<enumeration value="KEY_FIELDS_VALIDATOR">
				<annotation>
					<appinfo>

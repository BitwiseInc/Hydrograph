					<appinfo>
						<jaxb:typesafeEnumMember name="IntegerOrParameterValidationRule" />
					</appinfo>
				</annotation>
			</enumeration>
			<enumeration value="TRANSFORM_SCHEMA_GRID_VALIDATOR">
